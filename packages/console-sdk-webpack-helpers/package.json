{
  "name": "@mia-platform/webpack-helpers-console-microfrontend",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "private": true,
  "main": "./build/src/index.js",
  "typings": "./build/src/index.d.ts",
  "module": "./src/index.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "rm -rf ./build && tsc --build",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "unit": "jest"
  },
  "dependencies": {
    "@mia-platform/console-sdk-microfrontend": "0.0.0",
    "vite-plugin-qiankun": "1.0.15"
  },
  "devDependencies": {
    "@types/ramda": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "typescript": "^5.3.3",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "21.1.6",
    "jsdom": "^23.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=v18.18.2"
  },
  "homepage": "https://github.com/mia-platform/console-sdk-microfrontend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mia-platform/console-sdk-microfrontend.git"
  },
  "bugs": {
    "url": "https://github.com/mia-platform/console-sdk-microfrontend/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "<rootDir>/build"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts)",
      "**/?(*.)+(spec|test).+(ts)"
    ]
  }
}