{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "outDir": "./build",
    "rootDir": "src/",
  },
  "exclude": [
    "./node_modules",
    "./coverage",
    "./build",
    "./**/*.test.ts",
    "tsconfig.json"
  ],
  "include": [
    "./**/*.ts",
    "./**/*.json",
    "../.eslintrc.cjs",
  ],
}
  