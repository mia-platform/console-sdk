{
  "name": "console-sdk",
  "license": "Apache-2.0",
  "private": true,
  "sideEffects": false,
  "workspaces": [
    "packages/microfrontend-sdk",
    "packages/vite-helpers-console-microfrontend",
    "packages/webpack-helpers-console-microfrontend"
  ],
  "devDependencies": {
    "@mia-platform/eslint-config-mia": "^3.0.0",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.2",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "dependencies": {},
  "scripts": {
    "sdk": "pnpm --filter @mia-platform/microfrontend-sdk",
    "vite-helper": "pnpm --filter @mia-platform/vite-helpers-console-microfrontend",
    "webpack-helper": "pnpm --filter @mia-platform/webpack-helpers-console-microfrontend",
    
    "build": "tsc --build --verbose",
    "test": "pnpm sdk test --coverage && pnpm run vite-helper test --coverage && pnpm run webpack-helper test --coverage",
    "lint:fix": "pnpm sdk run lint --fix && pnpm run vite-helper run lint --fix && pnpm run webpack-helper run lint --fix",
    "lint": "pnpm run sdk run lint && pnpm run vite-helper run lint && pnpm run webpack-helper run lint",

    "addlicense": "addlicense -c 'Mia srl' -s -ignore packages/**/*.ts",
    "check-license": "addlicense -c 'Mia srl' -check -s -ignore packages/**/*.ts",
    "changeset": "changeset"
  },
  "version": "0.0.0",
  "contributors": [
    "Daniele Cin√† <daniele.cina@mia-platform.eu>",
    "Davide Bianchi <davide.bianchi@mia-platform.eu>",
    "Federico Maggi <federico.maggi@mia-platform.eu>"
  ]
}
